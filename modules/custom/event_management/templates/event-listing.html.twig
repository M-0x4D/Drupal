<div class="card-deck">
  {% for event in events %}
    <div class="card">
      <img src="{{ event.image }}" class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">{{ event.title }}</h5>
        <p class="card-text">{{ event.description }}</p>
      </div>
      <a href="{{ path('event_management.event_detail', {'event_id': event.id}) }}" class="btn btn-primary">->
        View</a>
    </div>
  {% endfor %}
</div>



